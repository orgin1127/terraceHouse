<link rel="stylesheet" href="css/app/inviteUser.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
</head>
<body>
<div id="searchDiv">
	<table>
		<tr>
			<td style="border-right:solid black 1px;">아이디 검색</td>
			<td><input type="text" id="searchId"></td>
			<td><input type="button" id="searchBtn" value="검색"></td>
		</tr>
	</table>
</div>

<br>
<div id="searchResult">
</div>
<form action="inviting" method="get" id="inviteForm">
	<input type="hidden" id="terrace_number" name ="terrace_number" value = "{{terrace_num}}">
	<input type="hidden" id="terrace_name" name = "terrace_name" value="{{terrace_name}}">
	<input type="hidden" id="userId" name="receiver">		
</form>
<script src="js/app/inviteUser.js"></script>
</body>
</html>